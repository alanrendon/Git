<table class="tabla_captura">
	<!-- START BLOCK : cia -->
	<tr>
		<th colspan="3" align="left" class="font12" scope="col">{num_cia} {nombre_cia}</th>
	</tr>
	<tr>
		<th>#</th>
		<th>Empleado</th>
		<th>Fecha</th>
	</tr>
	<!-- START BLOCK : emp -->
	<tr class="linea_{row_color}">
		<td align="right">{num_emp}</td>
		<td>{nombre_emp}</td>
		<td align="center">{fecha}</td>
	</tr>
	<!-- END BLOCK : emp -->
	<tr>
		<td colspan="3">&nbsp;</td>
	</tr>
	<!-- END BLOCK : cia -->
</table>
<p>
	<input type="button" name="cerrar" id="cerrar" value="Cerrar" />
</p>
