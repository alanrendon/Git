<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
"http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<title>Untitled Document</title>
<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">
<link href="../../styles/impresion.css" rel="stylesheet" type="text/css">
<link href="../../styles/tablas.css" rel="stylesheet" type="text/css">
<link href="../../styles/pages.css" rel="stylesheet" type="text/css">
</head>

<body>
<table width="100%"  height="100%" border="0" cellspacing="0" cellpadding="0" align="center">
<tr>
<td align="center" valign="middle"><table class="print">
  <tr>
    <th colspan="2" class="print" scope="row">Compa&ntilde;&iacute;a</th>
    </tr>
  <tr>
    <td colspan="2" class="print" scope="row">{num_cia} - {nombre_cia} </td>
    </tr>
  <tr>
    <th colspan="2" class="print" scope="row">Nombre y n&uacute;mero de empleado </th>
    </tr>
  <tr>
    <td class="print" scope="row">{num_emp}</td>
    <td class="print">{nombre_emp}</td>
  </tr>
</table>

  <br>
  <table class="print">
    <tr>
      <th class="print" scope="col">Fecha</th>
      <th class="print" scope="col">Importe del prestamo </th>
    </tr>
    <tr>
      <td class="print">{fecha_prestamo}</td>
      <td class="print">{importe_prestamo}</td>
    </tr>
  </table>
  <br>
  <table class="print">
    <tr>
      <th class="print" scope="col">Fecha</th>
      <th class="print" scope="col">Abonos</th>
    </tr>
    <tr>
      <td class="print">{fecha_abonos}</td>
      <td class="print">{importe_abonos}</td>
    </tr>
  </table>
  <p>
    <input type="button" value="Cerrar" class="boton">
  </p></td>
</tr>
</table>
</body>
</html>
