<table class="tabla_captura">
	<!-- START BLOCK : cia -->
	<tr>
		<th colspan="3" align="left" class="font12" scope="col">{num_cia} {nombre_cia}</th>
	</tr>
	<tr>
		<th>Fecha</th>
		<th>Importe</th>
		<th><img src="/lecaroz/imagenes/tool16x16.png" name="alta_inm" width="16" height="16" id="alta_arr" /></th>
	</tr>
	<!-- START BLOCK : row -->
	<tr id="row{id}" class="linea_{color}">
		<td align="center" nowrap="nowrap">{fecha}</td>
		<td align="right">{importe}</td>
		<td align="center" nowrap="nowrap"><img src="/lecaroz/iconos/magnify.png" alt="{id}" name="ver" width="16" height="16" id="ver" />&nbsp;&nbsp;<img src="/lecaroz/iconos/envelope.png" alt="{id}" name="enviar" width="16" height="16" id="enviar" /></td>
	</tr>
	<!-- END BLOCK : row -->
	<tr>
		<td colspan="3">&nbsp;</td>
	</tr>
	<!-- END BLOCK : cia -->
</table>
<p>
	<input type="button" name="regresar" id="regresar" value="Regresar" />
</p>
