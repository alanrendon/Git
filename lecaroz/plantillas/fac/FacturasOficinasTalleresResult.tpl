<table class="tabla_captura">
  <!-- START BLOCK : emisor -->
  <tr>
	<th colspan="5" align="left" class="font14" scope="col">{emisor}</th>
	</tr>
  <tr>
    <th>Folio</th>
	<th>Compa&ntilde;&iacute;a</th>
	<th>Mes</th>
	<th>Status</th>
	<th>Importe</th>
  </tr>
  <!-- START BLOCK : row -->
  <tr id="row">
    <td align="right">{folio}</td>
	<td>{num_cia} {nombre_cia}</td>
	<td>{mes} {anio}</td>
	<td>{status}</td>
	<td align="right">{importe}</td>
  </tr>
  <!-- END BLOCK : row -->
  <tr>
	<th colspan="4" align="right">Total</th>
	<th align="right" class="font12">{total}</th>
  </tr>
  <tr>
	<td colspan="5">&nbsp;</td>
	</tr>
  <!-- END BLOCK : emisor -->
</table>
