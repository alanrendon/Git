<table class="table">
	<thead>
		<tr>
			<th scope="col">Folio</th>
			<th scope="col">Fecha</th>
			<th scope="col">P&oacute;lizas</th>
			<th scope="col"><img src="/lecaroz/imagenes/tool16x16.png" width="16" height="16" /></th>
		</tr>
	</thead>
	<tbody>
		<!-- START BLOCK : row -->
		<tr>
			<td class="right">{folio}</td>
			<td class="center">{fecha}</td>
			<td class="right">{polizas}</td>
			<td class="center"><img src="/lecaroz/iconos/magnify.png" alt="{folio}" width="16" height="16" class="icono" id="mostrar" /></td>
		</tr>
		<!-- END BLOCK : row -->
	</tbody>
	<tfoot>
		<tr>
			<td colspan="4">&nbsp;</td>
		</tr>
	</tfoot>
</table>
<p><input name="regresar" type="button" id="regresar" value="Regresar" /></p>
